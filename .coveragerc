[run]
source = django_minify_compress_staticfiles
parallel = true
concurrency = multiprocessing
omit =
    */test*
    */__init__.py
    */migrations/*
    */tests/*
    django_minify_compress_staticfiles/__init__.py

[report]
show_missing = True
skip_covered = False
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:

[html]
directory = htmlcov